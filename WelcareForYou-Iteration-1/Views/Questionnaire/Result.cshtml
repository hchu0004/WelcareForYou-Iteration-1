                <td>
                    @Html.DisplayFor(modelItem => item.MediumPrice)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.NumOfRoom)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.HouseType)
                </td>
            </tr>
            }
        </tbody>
    </table>
</div>

