@model IEnumerable<WelcareForYou_Iteration_1.Models.Housing>
@{
    ViewBag.Title = "Result";
}


<header id="header">
    <div class="intro">
        <div class="overlay">
            <div class="container">
                <div class="row">
                    <div class="col-md-8 col-md-offset-2 intro-text">
                        <h1>
                            @ViewBag.Title
                        </h1>

                    </div>
                </div>
            </div>
        </div>
    </div>
</header>

<div class="container">
    <table id="table" class="display" style="width:100%">
        <thead>
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.Suburb)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.MediumPrice)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.NumOfRoom)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.HouseType)
                </th>
                <th></th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
            <tr>
